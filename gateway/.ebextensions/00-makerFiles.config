files:
  "/sbin/appstart":
    mode: "000755"
    owner: webapp
    group: webapp
    content: |
      #!/usr/bin/env bash
      JAR_PATH=/var/app/current/application.jar

      # Kill existing java processes
      killall java || true

      # Run the application
      java -Dfile.encoding=UTF-8 -jar $JAR_PATH
